<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>

        <Console name="StdOut" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{ISO8601}{GMT+0} (%t) %m%n"/>
        </Console>

    </Appenders>

    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="StdOut" level="DEBUG"/>
        </Root>
    </Loggers>

</Configuration>